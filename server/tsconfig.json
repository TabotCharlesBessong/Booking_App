{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2016",
    "noImplicitAny": true,
    "removeComments": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./src",
    "outDir": "./dix",
    "baseUrl": ".",
   // "tsx": "react-tsx", 
   "allowImportingTsExtensions": true,
   "noEmit": true,
    "jsxFactory": "React.createElement",
  }

}


